{ 
  "manifest_version": 2,
  "name": "Notas S - Territorio",
  "description": "By JK",
  "version": "1.0",
  "browser_action": {
       "default_icon": "icon.png"
  },
  "icons": {
        "128": "icon.png",
        "16": "icon.png"
      },
  "content_scripts": [
    {
      "matches": [
        "*://sena.territorio.la/perfil.php?id=*","*://sena.territorio.la/perfil.php?id=*"
      ],
      "js": ["jquery.min.js","bootstrap.min.js", "content.js"]
    }
  ],
  "permissions": [ "activeTab", "downloads", "*://sena.territorio.la/perfil.php?id=*","*://sena.territorio.la/perfil.php?id=*"]
}
